#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/C_PROJECTS/2022-itb1704-isa4/FrontEnd/node_modules/.pnpm/semver@7.3.5/node_modules/semver/bin/node_modules:/mnt/c/C_PROJECTS/2022-itb1704-isa4/FrontEnd/node_modules/.pnpm/semver@7.3.5/node_modules/semver/node_modules:/mnt/c/C_PROJECTS/2022-itb1704-isa4/FrontEnd/node_modules/.pnpm/semver@7.3.5/node_modules:/mnt/c/C_PROJECTS/2022-itb1704-isa4/FrontEnd/node_modules/.pnpm/node_modules:/mnt/c/C_PROJECTS/2022-itb1704-isa4/FrontEnd/node_modules:/mnt/c/C_PROJECTS/2022-itb1704-isa4/node_modules:/mnt/c/C_PROJECTS/node_modules:/mnt/c/node_modules:/mnt/c/C_PROJECTS/2022-itb1704-isa4/FrontEnd/node_modules/.pnpm/vscode-typescript-languageservice@0.29.8/node_modules/vscode-typescript-languageservice/node_modules:/mnt/c/C_PROJECTS/2022-itb1704-isa4/FrontEnd/node_modules/.pnpm/vscode-typescript-languageservice@0.29.8/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/C_PROJECTS/2022-itb1704-isa4/FrontEnd/node_modules/.pnpm/semver@7.3.5/node_modules/semver/bin/node_modules:/mnt/c/C_PROJECTS/2022-itb1704-isa4/FrontEnd/node_modules/.pnpm/semver@7.3.5/node_modules/semver/node_modules:/mnt/c/C_PROJECTS/2022-itb1704-isa4/FrontEnd/node_modules/.pnpm/semver@7.3.5/node_modules:/mnt/c/C_PROJECTS/2022-itb1704-isa4/FrontEnd/node_modules/.pnpm/node_modules:/mnt/c/C_PROJECTS/2022-itb1704-isa4/FrontEnd/node_modules:/mnt/c/C_PROJECTS/2022-itb1704-isa4/node_modules:/mnt/c/C_PROJECTS/node_modules:/mnt/c/node_modules:/mnt/c/C_PROJECTS/2022-itb1704-isa4/FrontEnd/node_modules/.pnpm/vscode-typescript-languageservice@0.29.8/node_modules/vscode-typescript-languageservice/node_modules:/mnt/c/C_PROJECTS/2022-itb1704-isa4/FrontEnd/node_modules/.pnpm/vscode-typescript-languageservice@0.29.8/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../semver@7.3.5/node_modules/semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../../../../../semver@7.3.5/node_modules/semver/bin/semver.js" "$@"
fi
